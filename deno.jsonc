{
    "importMap": "import_map.json",
    "tasks": {
        "build": "               deno run --lock --allow-write=./out --allow-read=./out,./src         ./main.ts build -p",
        "watch": "rm -r ./out && deno run --lock --allow-write=./out --allow-read=./out,./src --watch ./main.ts watch"
    }
}
